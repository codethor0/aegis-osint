{
  "name": "aegis-osint",
  "version": "1.0.1",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "description": "Open Intelligence for a Networked World",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "validate:data": "tsx scripts/validate-build.ts",
    "validate:links": "tsx scripts/validate-links.ts",
    "validate:hygiene": "tsx scripts/check-hygiene.ts",
    "security:audit": "npm audit --audit-level=high --omit=dev"
  },
  "keywords": [
    "osint",
    "open-source-intelligence",
    "security",
    "research"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "next": "^14.2.35",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jsdom": "^27.2.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.21.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.15"
  }
}
